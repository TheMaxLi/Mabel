<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mabel</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
   <%- include('header'); %>
      <div class="cards-container">
        <% for (let card of info.cards) { %>
          <%- include('card', {card : card}) %>
        <% } %>
        <% if (info.req.session.user) { %> 
          <%- include("addArticle", {category : info.req.params.category}) %> 
         <% } %>
      </div>

      <%- include('footer') %>
      </body>
</body>
</html>